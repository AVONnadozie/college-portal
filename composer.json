{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "jrm2k6/cloudder": "^0.6.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "mpociot/laravel-apidoc-generator": "^2.0",
        "tymon/jwt-auth": "0.5.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "gh:deploy": [
            "git checkout edge",
            "git subtree split --prefix public/docs -b gh-pages",
            "touch public/docs/.gitignore",
            "grep -v \"public/docs\" .gitignore >> public/docs/.gitignore",
            "git checkout gh-pages",
            "mv public/docs/.gitignore .gitignore",
            "git add .",
            "git commit -m \"chore: generate gh-pages\"",
            "git subtree push -f --prefix public/docs origin gh-pages:gh-pages",
            "git checkout edge",
            "git branch -D gh-pages"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
